	.arch   armv7-a
	.file   "/mnt/e/workspace_clion/compiler_2020/sysyruntimelibrary-master/functional_test/50_recursion_test1.sy"
	.data
	.text
	.align  2
	.global fact
	.type   fact, %function
fact:
	str     fp, [sp, #-4]!
	add     FP, SP, #0
	sub     SP, SP, #8
	ldr     R10, [FP, #4]
	mov     R1, #0
	cmp     R10, R1
	beq     .L0
	b       .L1
.L1:
	ldr     R1, [FP, #4]
	sub     R1, R1, #1
	str     R1, [FP, #-8]
	ldr     R1, [FP, #4]
# 	mov     R1, R1
	ldr     R2, [FP, #-8]
	str     R2, [SP]
	bl      fact
	mov     R10, R0
	mul     R10, R1, R10
	mov     R0, R10
	b       .L2
.L0:
	mov     R10, #1
	mov     R0, R10
	b       .L1
.L2:
NOP
	sub     sp, fp, #0
	ldr     fp, [sp], #4
	bx      lr
	.size   fact, .-fact
	.align  2
	.global main
	.type   main, %function
main:
	stmfd   sp!, {fp, lr}
	add     FP, SP, #4
	sub     SP, SP, #8
	mov     R1, #4
	str     R1, [FP, #-8]
	ldr     R1, [FP, #-8]
	str     R1, [SP]
	bl      fact
# 	mov     R0, R0
	b       .L3
.L3:
NOP
	sub     sp, fp, #4
	ldmfd   sp!, {fp, lr}
	bx      lr
	.size   main, .-main
